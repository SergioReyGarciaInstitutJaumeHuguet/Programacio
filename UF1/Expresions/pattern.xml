<?xml version="1.0" encoding="UTF-8" ?>

<patrons xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="14-patterns.xsd">

    <!-- de 5 a 8 números -->

    <patroA>12345678</patroA>

    <patroA>99999999</patroA>

    <!-- 5 lletras majúscules -->

    <patroB>LMSGI</patroB>

    <!-- 3 lletres majúscules seguit de 3 minúscules -->

    <patroC>ABCdef</patroC>

    <!-- que tenga 3 letras mayúsculas o minúsculas de la a a la d, seguido de 2 números -->

    <patroD>AdC99</patroD>

    <patroD>bBb11</patroD>

    <!-- que tenga 3 números de 0 a 5 seguido de una letra minúscula desde a - d -->

    <patroE>123a</patroE>

    <!-- tenga 3 bloque de 3 números separados por - -->

    <patroF>123-456-789</patroF>

    <!-- empiece por una letra y tenga 2 números -->

    <patroG>c18</patroG>

    <!-- Quiero la palabra LGA, seguido de un guión, seguido de 4 dígitos  -->

    <patroH>LGA-1115</patroH>

    <!-- Quiero 1 número - cualquier caracter - 1 letra mayúscula  -->

    <patroI>3#P</patroI>

    <patroI>9$Q</patroI>

    <!-- Quiero la palabra LGA, seguido de un guión, seguido de 4 dígitos .V y número  -->

    <patroJ>LGA-1115.V3</patroJ>

    <!-- a o b, 1 ó 2, un guión, de 3 a 5 letras -->

    <patroK>a2-aaa</patroK>

    <patroK>b1-ababa</patroK>

    <!-- al menos una a, al menos una b, 0 o muchas c -->

    <patroL>aaabbbbb</patroL>

    <patroL>abcccccc</patroL>

</patrons>