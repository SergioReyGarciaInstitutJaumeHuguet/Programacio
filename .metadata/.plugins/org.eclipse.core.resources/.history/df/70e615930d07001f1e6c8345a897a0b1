
public class Velers extends Vaixell{

	private int pals;
	
	public Velers(String matricula, double eslora, int any, int pals) {
		super();
		this.matricula = matricula;
		this.eslora = eslora;
		this.any = any;
	}
	
	public double importAssociatAlVaixell() {
		double preuBase = (this.eslora * 10) + this.pals;
		return preuBase;
	}
}
