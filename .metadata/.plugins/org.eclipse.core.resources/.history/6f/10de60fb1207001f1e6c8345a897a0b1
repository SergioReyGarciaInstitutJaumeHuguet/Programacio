package Prueba;
import java.time.LocalDate;
import java.time.format.DateTimeFormatter;
import java.time.temporal.ChronoUnit;
public class prueba {
	String dataInicial = "2005/10/21";
	String dataFinal = "2005/11/21";
	private DateTimeFormatter form = DateTimeFormatter.ofPattern("yyyy/MM/dd");;
	LocalDate data1 = LocalDate.parse(dataInicial,form);
	LocalDate data2 = LocalDate.parse(dataFinal,form);
	long dies = ChronoUnit.DAYS.between(data1, data2);
	
	public static void main(String[] args) {
		System.out.println(data1);

	}

}
